{
	"editor.bracketPairColorization.enabled": true,
	"editor.guides.bracketPairs": "active",
	"editor.formatOnSave": true,
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
	"editor.detectIndentation": false,
	"editor.insertSpaces": false,
	"eslint.run": "onSave",
	"eslint.useESLintClass": true,
	"eslint.format.enable": true,
	"eslint.validate": [
		"javascript",
		"typescript",
	],
	"prettier.enable": false,
	"[typescript]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint"
	},
	"typescript.format.insertSpaceAfterTypeAssertion": true,
	"typescript.format.semicolons": "insert",
	"typescript.preferences.importModuleSpecifier": "project-relative",
	"typescript.preferences.quoteStyle": "single",
	"typescript.updateImportsOnFileMove.enabled": "always",
	"javascript.preferences.importModuleSpecifierEnding": "js",
	"typescript.preferences.importModuleSpecifierEnding": "js",
	"typescript.tsdk": "node_modules\\typescript\\lib",
	"workbench.colorCustomizations": {
		"activityBar.activeBackground": "#872b2b",
		"activityBar.background": "#872b2b",
		"activityBar.foreground": "#e7e7e7",
		"activityBar.inactiveForeground": "#e7e7e799",
		"activityBarBadge.background": "#081808",
		"activityBarBadge.foreground": "#e7e7e7",
		"commandCenter.border": "#e7e7e799",
		"sash.hoverBorder": "#872b2b",
		"statusBar.background": "#601f1f",
		"statusBar.foreground": "#e7e7e7",
		"statusBarItem.hoverBackground": "#872b2b",
		"statusBarItem.remoteBackground": "#601f1f",
		"statusBarItem.remoteForeground": "#e7e7e7",
		"titleBar.activeBackground": "#601f1f",
		"titleBar.activeForeground": "#e7e7e7",
		"titleBar.inactiveBackground": "#601f1f99",
		"titleBar.inactiveForeground": "#e7e7e799"
	},
	"peacock.color": "#601f1f",
}